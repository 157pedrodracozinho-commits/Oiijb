btn.Font = Enum.Font.GothamBold
btn.TextSize = 14
btn.TextColor3 = Color3.fromRGB(255,255,255)
btn.Text = name
Instance.new("UICorner", btn).CornerRadius = UDim.new(0,6)
return btn
end

local function fireRolePlayName(target)
local ok,err = pcall(function()
local RE = ReplicatedStorage:FindFirstChild("RE")
assert(RE,"RE folder not found in ReplicatedStorage")
local Skibidi1 = RE:FindFirstChild("1RPNam1eTex1t")
assert(Skibidi1 and Skibidi1:IsA("RemoteEvent"),"RemoteEvent 1RPNam1eTex1t not found")
Skibidi1:FireServer("RolePlayName",target)
end)
if ok then
game.StarterGui:SetCore("SendNotification",{Title="Executado", Text="RolePlayName definido para: "..target, Duration=3})
else
game.StarterGui:SetCore("SendNotification",{Title="Erro", Text="Erro ao definir: "..tostring(err), Duration=5})
end
end

