-- dropdown visual
local dropdownBtn = Instance.new("TextButton", charScroll)
dropdownBtn.Size = UDim2.new(0,200,0,28)
dropdownBtn.Text = "Escolher skin..."
dropdownBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
dropdownBtn.TextColor3 = Color3.fromRGB(255,255,255)
dropdownBtn.Font = Enum.Font.Gotham
dropdownBtn.TextSize = 14
Instance.new("UICorner", dropdownBtn).CornerRadius = UDim.new(0,6)

local dropFrame = Instance.new("Frame", charScroll)
dropFrame.Size = UDim2.new(0,200,0,0)
dropFrame.BackgroundColor3 = Color3.fromRGB(40,40,40)
dropFrame.ClipsDescendants = true
Instance.new("UICorner", dropFrame).CornerRadius = UDim.new(0,6)

local listLayout = Instance.new("UIListLayout", dropFrame)
listLayout.Padding = UDim.new(0,4)
listLayout.SortOrder = Enum.SortOrder.LayoutOrder

local selectedSkin = nil
local dropdownOpen = false

local function populateDropdown()
-- limpar filhos (exceto UIListLayout)
for _,c in ipairs(dropFrame:GetChildren()) do
if not c:IsA("UIListLayout") then c:Destroy() end
end
for i,v in ipairs(skins) do
local item = Instance.new("TextButton", dropFrame)
item.Size = UDim2.new(1, -10, 0, 28)
item.Position = UDim2.new(0, 5, 0, (i-1)*32)
item.BackgroundColor3 = Color3.fromRGB(55,55,55)
item.Font = Enum.Font.Gotham
item.Text = v.name
item.TextColor3 = Color3.fromRGB(255,255,255)
item.TextSize = 14
Instance.new("UICorner", item).CornerRadius = UDim.new(0,6)
item.MouseButton1Click:Connect(function()
selectedSkin = v
dropdownBtn.Text = "Skin: " .. v.name
-- fechar lista
dropFrame:TweenSize(UDim2.new(0,200,0,0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.15, true)
dropdownOpen = false
end)
end
end

dropdownBtn.MouseButton1Click:Connect(function()
if dropdownOpen then
dropFrame:TweenSize(UDim2.new(0,200,0,0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.15, true)
dropdownOpen = false
else
populateDropdown()
local itemHeight = 32
local num = math.min(#skins, 6)
local height = num * itemHeight + 8
dropFrame:TweenSize(UDim2.new(0,200,0,height), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.15, true)
dropdownOpen = true
end
end)

-- bot√£o executar: aqui executamos exatamente os scripts que voc√™ enviou (InvokeServer(id))
local execBtn = Instance.new("TextButton", charScroll)
execBtn.Size = UDim2.new(0,200,0,28)
execBtn.Text = "Executar Skin"
execBtn.BackgroundColor3 = Color3.fromRGB(70,70,70)
execBtn.TextColor3 = Color3.fromRGB(255,255,255)
execBtn.Font = Enum.Font.GothamBold
execBtn.TextSize = 14
Instance.new("UICorner", execBtn).CornerRadius = UDim.new(0,6)

execBtn.MouseButton1Click:Connect(function()
if not selectedSkin then
game.StarterGui:SetCore("SendNotification",{Title="Erro",Text="Escolha uma skin antes de executar.",Duration=3})
return
end

-- tentar chamar o RemoteFunction LoadOutfit
local ok, err = pcall(function()
-- tentar obter RemoteFunction
local remotesFolder = ReplicatedStorage:FindFirstChild("Remotes")
assert(remotesFolder, "Remotes not found in ReplicatedStorage")
local LoadOutfit = remotesFolder:FindFirstChild("LoadOutfit")
assert(LoadOutfit and LoadOutfit:IsA("RemoteFunction"), "LoadOutfit RemoteFunction not found")
-- executar exatamente o que voc√™ enviou: InvokeServer(id)
LoadOutfit:InvokeServer(selectedSkin.id)
end)

if ok then
game.StarterGui:SetCore("SendNotification",{Title="Executado",Text="Script da skin "..selectedSkin.name.." executado.",Duration=3})
else
game.StarterGui:SetCore("SendNotification",{Title="Erro",Text="Falha ao executar: "..tostring(err),Duration=5})
end

end)

