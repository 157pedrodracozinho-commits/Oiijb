-- Cria o bot√£o RGB
local rgbBtn = Instance.new("TextButton", biuScroll)
rgbBtn.Size = UDim2.new(0, 150, 0, 32)
rgbBtn.BackgroundColor3 = Color3.fromRGB(110,40,100)
rgbBtn.Font = Enum.Font.GothamBold
rgbBtn.TextSize = 15
rgbBtn.TextColor3 = Color3.fromRGB(255,255,255)
rgbBtn.Text = "RGB"
Instance.new("UICorner", rgbBtn).CornerRadius = UDim.new(0,8)

-- Controle de execu√ß√£o (thread)
local rgbRunning = false
local rgbThread = nil

local function fireRPColor(color)
    local RE = game:GetService("ReplicatedStorage"):FindFirstChild("RE")
    if not RE then return end
    local evt = RE:FindFirstChild("1RPNam1eColo1r")
    if evt and evt:IsA("RemoteEvent") then
        evt:FireServer("PickingRPNameColor", color)
    end
end

rgbBtn.MouseButton1Click:Connect(function()
    if not rgbRunning then
        rgbRunning = true
        rgbBtn.Text = "Parar RGB"
        rgbThread = coroutine.create(function()
            while rgbRunning do
                local delayMs = tonumber(rgbSpeedBox.Text) or 120
                for _, clr in ipairs(rgbColors) do
                    fireRPColor(clr)
                    wait(delayMs / 1000)
                    if not rgbRunning then break end
                end
            end
        end)
        coroutine.resume(rgbThread)
    else
        rgbRunning = false
        rgbBtn.Text = "RGB"
    end
end)
