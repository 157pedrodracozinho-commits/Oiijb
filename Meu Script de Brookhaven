local ok = pcall(function() setclipboard(discordLink) end)
if ok then
game:GetService("StarterGui"):SetCore("SendNotification", {Title="Copiado!", Text="Link do Discord copiado.", Duration=3})
else
game:GetService("StarterGui"):SetCore("SendNotification", {Title="Erro", Text="N√£o foi poss√≠vel copiar automaticamente. Copie manualmente: "..discordLink, Duration=5})
end
end)
end

