-- helper para criar se√ß√µes e controles dentro do scroll
local function createSectionTitle(text)
local t = Instance.new("TextLabel", charScroll)
t.Size = UDim2.new(1, -20, 0, 22)
t.BackgroundTransparency = 1
t.Font = Enum.Font.GothamBold
t.TextColor3 = Color3.fromRGB(255,255,255)
t.Text = text
t.TextXAlignment = Enum.TextXAlignment.Left
return t
end
local function createInput(placeholder)
local tb = Instance.new("TextBox", charScroll)
tb.Size = UDim2.new(0,200,0,28)
tb.PlaceholderText = placeholder
tb.BackgroundColor3 = Color3.fromRGB(50,50,50)
tb.TextColor3 = Color3.fromRGB(255,255,255)
tb.Font = Enum.Font.Gotham
tb.TextSize = 14
Instance.new("UICorner", tb).CornerRadius = UDim.new(0,6)
return tb
end
local function createSmallButton(text)
local b = Instance.new("TextButton", charScroll)
b.Size = UDim2.new(0,120,0,28)
b.BackgroundColor3 = Color3.fromRGB(70,70,70)
b.Font = Enum.Font.GothamBold
b.TextSize = 14
b.TextColor3 = Color3.fromRGB(255,255,255)
b.Text = text
Instance.new("UICorner", b).CornerRadius = UDim.new(0,6)
return b
end

-- Speed / Jump / Infinite / NoClip (mantidos)
createSectionTitle("Speed")
local speedBox = createInput("Velocidade (ex: 50)")
local speedBtn = createSmallButton("Ativar")
local speedOn = false
local function getHumanoid()
if player.Character then
return player.Character:FindFirstChildOfClass("Humanoid")
end
return nil
end
speedBtn.MouseButton1Click:Connect(function()
local hum = getHumanoid()
if not hum then return end
speedOn = not speedOn
if speedOn then
hum.WalkSpeed = tonumber(speedBox.Text) or 16
speedBtn.Text = "Desligar"
else
hum.WalkSpeed = 16
speedBtn.Text = "Ativar"
end
end)

createSectionTitle("Jump")
local jumpBox = createInput("For√ßa do pulo (ex: 100)")
local jumpBtn = createSmallButton("Ativar")
local jumpOn = false
jumpBtn.MouseButton1Click:Connect(function()
local hum = getHumanoid()
if not hum then return end
jumpOn = not jumpOn
if jumpOn then
hum.UseJumpPower = true
hum.JumpPower = tonumber(jumpBox.Text) or 50
jumpBtn.Text = "Desligar"
else
hum.JumpPower = 50
jumpBtn.Text = "Ativar"
end
end)

createSectionTitle("Infinite Jump")
local infBtn = createSmallButton("Ligar")
local infOn = false
UserInputService.JumpRequest:Connect(function()
if infOn then
local hum = getHumanoid()
if hum then hum:ChangeState("Jumping") end
end
end)
infBtn.MouseButton1Click:Connect(function()
infOn = not infOn
infBtn.Text = infOn and "Desligar" or "Ligar"
end)

createSectionTitle("No Clip")
local noclipBtn = createSmallButton("Ligar")
local noclipOn = false
RunService.Stepped:Connect(function()
if noclipOn and player.Character then
for _, part in pairs(player.Character:GetDescendants()) do
if part:IsA("BasePart") then part.CanCollide = false end
end
end
end)
noclipBtn.MouseButton1Click:Connect(function()
noclipOn = not noclipOn
noclipBtn.Text = noclipOn and "Desligar" or "Ligar"
end)

